                                output_items[0][retcnt:retcnt+self.gap] = 0
                                retcnt = retcnt + self.gap         
                                
                        self.sent = 1
                        self.send = 0 #forresten det kanske funka bara att det inte sags pa time sink
                        #print self.sent
        elif ~self.send &amp; 0x01:
                self.sent = 0
